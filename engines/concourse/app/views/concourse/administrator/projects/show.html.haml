.block-header
  %h2
    Detalhes de concurso
.card
  .card-header
    %span.pull-right
      = link_to "voltar para concursos", concourse.administrator_projects_path
    %h2 
      = @project.name
      %small
        = @project.description
  .card-body.card-padding
    %div{:role => "tabpanel"}
      %ul.tab-nav{:role => "tablist", :style => "overflow: hidden; outline: none;", :tabindex => "1"}
        %li.active
          %a{"aria-controls" => "subscribes", "aria-expanded" => "false", "data-toggle" => "tab", :href => "#subscribes", :role => "tab1"} Inscritos
        %li
          %a{"aria-controls" => "consults", "aria-expanded" => "false", "data-toggle" => "tab", :href => "#consults", :role => "tab2"} Consultas
        %li
          %a{"aria-controls" => "projects", "aria-expanded" => "true", "data-toggle" => "tab", :href => "#projects", :role => "tab3"} Projetos enviados
      .tab-content
        #subscribes.tab-pane.active{:role => "tab1"}
          %p
            Inscrito
        #consults.tab-pane{:role => "tab2"}
          %p
            consultas
        #projects.tab-pane{:role => "tab2"}
          %p
            Projetos enviados