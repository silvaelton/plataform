- @sectors.each_with_index do |j, i|
 %tr
  %td.col-md-1
   = i + 1
  %td
   = j.name
  %td
   = j.acron
  %td
   = j.father_id
  %td
   = j.responsible
  %td.col-md-2
   = link_to "", edit_sector_path(j), remote: true, class: 'md-mode-edit btn btn-primary waves-effect waves-button waves-float'
   = link_to "", sector_path(j), method: :delete, class: 'md-highlight-remove btn btn-danger waves-effect waves-button waves-float', data: {confirm: "#{t(:confirm_delete)}"}
