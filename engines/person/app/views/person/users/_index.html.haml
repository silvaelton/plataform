- @users.each_with_index do |j, i|
 %tr
  %td.col-md-1
   = i + 1
  %td
   = j.name
  %td
   = j.code
  %td
   = j.status
  %td.col-md-2
   = link_to "", edit_user_path(j), class: 'md-mode-edit btn btn-primary waves-effect waves-button waves-float'
   = link_to "", user_path(j), method: :delete, class: 'md-highlight-remove btn btn-danger waves-effect waves-button waves-float', data: {confirm: "#{t(:confirm_delete)}"}
