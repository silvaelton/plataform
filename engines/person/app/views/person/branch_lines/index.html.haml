.card
  .card-header.ch-alt
    %h2
      Ramal
      = link_to 'novo Ramal', new_branch_line_path, remote: true, class: 'btn btn-success pull-right'
      %small Listagem de Ramal
  .table-responsive{:style => "overflow: hidden; outline: none;", :tabindex => "3"}
    %table.table.table-hover.table-bordered
      %thead
        %tr
          %th #
          %th Nome
          %th Ramal
          %th Status
          %th Ações
      %tbody.index-branch_line
        = render 'index'
